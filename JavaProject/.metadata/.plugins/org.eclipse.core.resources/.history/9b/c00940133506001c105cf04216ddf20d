package ch10;

public class MainTest1 {

	public static void main(String[] args) {
		
		// 질럿 생성
		Zealot zealot1 = new Zealot("질럿1");
		zealot1.showInfo();
		// 마린 생성
		Marine marine1 = new Marine("마린1");
		marine1.showInfo();
		// 저글링 생성
		Zergling zergling = new Zergling("저글링");
		zergling.showInfo();
	}

}
