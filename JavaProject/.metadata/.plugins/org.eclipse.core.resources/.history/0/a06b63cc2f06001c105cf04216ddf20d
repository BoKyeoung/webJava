package ch10;

public class Zergling {

	private String name;
	private int power;
	private int hp;

	// 생성자, 공격력 : 5, 체력 : 100
	public Zergling(String name) {
		this.name = name;
		this.power = 5;
		this.hp = 100;
	}

	// getter
	public String getName() {
		return this.name;
	}
	public int getPower() {
		return this.power;
	}
	public int getHp() {
		return this.hp;
	}

	// showInfo
	

}
